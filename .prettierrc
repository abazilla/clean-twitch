{
	"useTabs": true,
	"printWidth": 100,
	"trailingComma": "es5",
	"singleQuote": false,
	"semi": false,
	"plugins": ["prettier-plugin-organize-imports", "prettier-plugin-tailwindcss"],
	"importOrder": ["^react$", "<THIRD_PARTY_MODULES>", "", "^@(/.*)$", "^[.]", ".css$"],
	"importOrderParserPlugins": ["typescript", "jsx", "decorators-legacy"]
}
